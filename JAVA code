import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner Sc = new Scanner(System.in);
	    int n = Sc.nextInt();
	    System.out.println("Enter numbers : "); 
	    int[] a= new int[n];
	    int i,j;
	    int temp=0;
	    for(i=0;i<n;i++){
	        a[i]=Sc.nextInt();
	    }
	    for(i=0;i<n;i++){
	            if(a[i]>temp){
	               temp=a[i];
	            }
	    }
		System.out.println("Maximum value : " + temp); 
	}
}
